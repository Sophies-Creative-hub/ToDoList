<%- include('header'); -%>
    <div class="box" id="heading">
        <h1>
            <%= listTitle %>
        </h1>
    </div>
    <div class="box">
        <% for(let i=0; i<newListToDos.length;i++){ %>
            <div class="item">
                <input type="checkbox">
               <p> <%=newListToDos[i]%></p>
            </div>
            <% } %>
            
            <form class="item" action="/" method="post">
                <input type="text" name="nextToDo" placeholder="Type in your next Task" autocomplete="off">
                <button type="submit" name="list" value= <%= listTitle %> >+</button>
    </div>

<%- include('footer'); -%>